   "source": [
    "a= np.array([[0,3],\n",
    "             [4,256]])\n",
    "def imstretch(a):\n",
    "    m = a.min();\n",
    "    M = a.max();\n",
    "    return np.uint8((256-1)/(M-m) * (a-m))\n",
    "imstretch(a)"
   ]
